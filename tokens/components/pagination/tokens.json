{
  "pagination": {
    "active": {
      "text": {
        "value": "{link.light.value}",
        "type": "color"
      },
      "background": {
        "value": "{color.blue.900.value}",
        "type": "color"
      }
    },
    "border": {
      "radius": {
        "value": "{border.radius.sm.value}",
        "type": "borderRadius"
      },
      "width": {
        "value": "{border.width.md.value}",
        "type": "borderWidth"
      }
    },
    "default": {
      "text": {
        "value": "{link.default.value}",
        "type": "color"
      }
    },
    "hover": {
      "background": {
        "value": "{color.blue.100.value}",
        "type": "color"
      },
      "text": {
        "value": "#0535d2",
        "type": "color"
      }
    },
    "focus": {
      "background": {
        "value": "{focus.background.value}",
        "type": "color"
      },
      "text": {
        "value": "{focus.text.value}",
        "type": "color"
      },
      "outline": {
        "width": {
          "value": "{outline.width.value}",
          "type": "other"
        }

      }
    },
    "font": {
      "value": {
        "fontFamily": "{fontFamilies.body}",
        "fontWeight": "{fontWeights.medium}",
        "lineHeight": "{lineHeights.text}",
        "fontSize": "{fontSizes.text}"
      },
      "type": "typography"
    },
    "list": {
      "endButton": {
        "padding": {
          "value": "{spacing.200.value} {spacing.150.value}",
          "type": "spacing"
        }
      }
    },
    "listitem": {
      "margin": {
        "value": "{spacing.100.value}",
        "type": "spacing"
      }
    },
    "mobile": {
      "list": {
        "border": {
          "value": "{link.default.value}",
          "type": "border"
        },
        "item": {
          "margin": {
            "value": ".125rem",
            "type": "spacing"
          }
        },
        "prevnext": {
          "margin": {
            "value": "{spacing.200.value} auto 0",
            "type": "spacing"
          }
        }
      }
    },
    "simple": {
      "label": {
        "fontWeight": {
          "value": "{fontWeights.regular.value}",
          "type": "fontWeights"
        }
      },
      "listitem": {
        "margin": {
          "value": "{spacing.100.value} {spacing.100.value} {spacing.200.value}",
          "type": "spacing"
        },
        "text": {
          "margin": {
            "value": "0 0 {spacing.50.value}",
            "type": "spacing"
          }
        }
      },
      "padding": {
        "value": "{spacing.200.value} {spacing.150.value}",
        "type": "spacing"
      }
    }
  }
}
