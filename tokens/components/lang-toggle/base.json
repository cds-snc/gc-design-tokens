{
  "lang-toggle": {
    "active": {
      "background": {
        "value": "{active.background.value}",
        "type": "color"
      },
      "text": {
        "value": "{active.text.value}",
        "type": "color"
      }
    },
    "default": {
      "text": {
        "value": "{link.default.value}",
        "type": "color"
      }
    },
    "focus": {
      "background": {
        "value": "{focus.background.value}",
        "type": "color"
      },
      "text": {
        "value": "{focus.text.value}",
        "type": "color"
      }
    },
    "font": {
      "value": {
        "fontFamily": "{fontFamilies.body}",
        "fontWeight": "{fontWeights.regular}",
        "lineHeight": "{lineHeights.text}",
        "fontSize": "{fontSizes.text}"
      },
      "type": "typography"
    },
    "hover": {
      "text": {
        "value": "{link.hover.value}",
        "type": "color"
      }
    },
    "padding": {
      "value": "{spacing.300.value}",
      "type": "spacing"
    }
  }
}
