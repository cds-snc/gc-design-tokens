{
  "details": {
    "default": {
      "text": {
        "value": "{link.default.value}",
        "type": "color"
      },
      "decoration": {
        "thickness": {
          "value": "{border.width.sm.value}",
          "type": "borderWidth"
        }
      }
    },
    "focus": {
      "background": {
        "value": "{focus.background.value}",
        "type": "color"
      },
      "text": {
        "value": "{focus.text.value}",
        "type": "color"
      },
      "border-radius": {
        "value": "{border.radius.sm.value}",
        "type": "other"
      },
      "box-shadow": {
        "value": "0 0 0 {border.width.md.value} {color.grayscale.0.value}",
        "type": "color"
      },
      "outline-offset": {
        "value": "{border.width.md.value}",
        "type": "borderWidth"
      },
      "outline": {
        "value": "{spacing.50.value} solid {focus.background.value}",
        "type": "other"
      }
    },
    "font": {
      "value": {
        "fontFamily": "{fontFamilies.body}",
        "fontWeight": "{fontWeights.regular}",
        "lineHeight": "{lineHeights.text}",
        "fontSize": "{fontSizes.text}"
      },
      "type": "typography"
    },
    "fontSmall": {
      "value": {
        "fontFamily": "{fontFamilies.body}",
        "fontWeight": "{fontWeights.regular}",
        "lineHeight": "{lineHeights.caption}",
        "fontSize": "{fontSizes.caption}"
      },
      "type": "typography"
    },
    "hover": {
      "text": {
        "value": "{link.hover.value}",
        "type": "color"
      },
      "decoration": {
        "thickness": {
          "value": "{border.width.md.value}",
          "type": "borderWidth"
        }
      }
    },
    "panel": {
      "border": {
        "color": {
          "value": "{border.default.value}",
          "type": "border"
        },
        "width": {
          "value": "{border.width.xl.value}",
          "type": "borderWidth"
        }
      },
      "margin": {
        "value": "{spacing.200.value} 0 0 0",
        "type": "spacing"
      },
      "padding": {
        "value": "{spacing.150.value} {spacing.450.value}",
        "type": "spacing"
      },
      "slotted": {
        "margin": {
