        "borderTopBottom": {
          "value": "{spacing.150.value}",
          "type": "spacing"
        },
        "left": {
          "value": "{spacing.100.value}",
          "type": "spacing"
        },
        "top": {
          "value": "{spacing.150.value}",
          "type": "spacing"
        }
      },
      "border": {
        "width": {
          "value": "{border.width.sm.value}",
          "type": "borderWidth"
        }
      },
      "padding": {
        "value": "{spacing.100.value} {spacing.100.value} {spacing.100.value} {spacing.450.value}",
        "type": "spacing"
      }
