{
  "font-size": {
    "300": {
      "value": "$typography.base.font-size.value / $typography.base.scale.value",
      "type": "fontSizes"
    },
    "400": {
      "value": "$typography.base.font-size.value",
      "type": "fontSizes"
    },
    "500": {
      "value": "$typography.base.font-size.value * $typography.base.scale.value",
      "type": "fontSizes"
    },
    "600": {
      "value": "$typography.base.font-size.value * $typography.base.scale.value^2",
      "type": "fontSizes"
    },
    "700": {
      "value": "$typography.base.font-size.value * $typography.base.scale.value^3",
      "type": "fontSizes"
    },
    "800": {
      "value": "$typography.base.font-size.value * $typography.base.scale.value^4",
      "type": "fontSizes"
    },
    "900": {
      "value": "$typography.base.font-size.value * $typography.base.scale.value^5",
      "type": "fontSizes"
    },
    "1000": {
      "value": "$typography.base.font-size.value * $typography.base.scale.value^6",
      "type": "fontSizes"
    }
  }
} 