{
  "font-size": {
    "400": {
      "value": "1 / $typography.type-scale.value * $typography.base-size.value",
      "type": "fontSizes"
    },
    "500": {
      "value": "1 * $typography.base-size.value",
      "type": "fontSizes"
    },
    "600": {
      "value": "1 * $typography.type-scale.value * $typography.base-size.value",
      "type": "fontSizes"
    },
    "700": {
      "value": "1 * $typography.type-scale.value^2 * $typography.base-size.value",
      "type": "fontSizes"
    },
    "800": {
      "value": "1 * $typography.type-scale.value^3 * $typography.base-size.value",
      "type": "fontSizes"
    },
    "900": {
      "value": "1 * $typography.type-scale.value^4 * $typography.base-size.value",
      "type": "fontSizes"
    },
    "1000": {
      "value": "1 * $typography.type-scale.value^5 * $typography.base-size.value",
      "type": "fontSizes"
    }
  }
}